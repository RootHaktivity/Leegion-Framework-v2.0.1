[flake8]
max-line-length = 120
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    *.pyc,
    *.pyo,
    *.pyd,
    .pytest_cache,
    .coverage,
    htmlcov,
    dist,
    build,
    *.egg-info,
    .venv,
    venv,
    env,
    .env
per-file-ignores =
    modules/command_helper.py: E501
    modules/reverse_shell_generator.py: E501
    modules/ssl_analyzer.py: E501
    modules/directory_bruteforce.py: E501 